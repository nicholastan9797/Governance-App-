    "prettier": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "update": "ncu -u"
  },
  "dependencies": {
    "@prisma/client": "4.15.0",
    "ethers": "^6.6.0",
    "exponential-backoff": "^3.1.1",
    "type-fest": "^3.12.0"
  },
  "devDependencies": {
    "@senate/config": "*",
    "eslint": "^8.42.0",
    "prisma": "4.15.0",
    "prisma-dbml-generator": "^0.10.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@1.22.19"
}
